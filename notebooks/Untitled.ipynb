    "from sklearn.decomposition import PCA\n",
    "import numpy as np\n",
    "\n",
    "# lets create features\n",
    "x1 = np.random.normal(size=200)\n",
    "x2 = np.random.normal(size=200)\n",
    "x3 = x1 + x2 #not useful since its highly correlated with other features.\n",
    "X = np.c_[x1,x2,x3]\n",
    "\n",
    "pca = PCA()\n",
    "pca.fit(X)"
   ]
